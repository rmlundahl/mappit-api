{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "awobaz/compoships": "^2.1",
        "enlightn/enlightn": "^2.10",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "intervention/imagecache": "^2.5",
        "laravel-lang/lang": "^11.0",
        "laravel/fortify": "^1.18",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.0",
        "maatwebsite/excel": "^3.1.48",
        "mavinoo/laravel-batch": "^2.3",
        "staudenmeir/laravel-adjacency-list": "^1.12"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "larastan/larastan": "^2.0",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Mappit\\ExtHvaindestad\\": "packages/ext-hvaindestad/src",
            "Mappit\\ExtLerenMetDeStad\\": "packages/ext-lerenmetdestad/src",
            "Mappit\\ExtShareMyStory\\": "packages/ext-sharemystory/src"
        },
        "files": [
            "app/Utils/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "phpstan": [
            "vendor/bin/phpstan analyse"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
